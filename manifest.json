{
  "manifest_version": 3,
  "name": "Tabs Home",
  "version": "1.0",
  "description": "Chrome tabs collection",
  "icons": {
    "128": "assets/icons/icon.png"
  },
  "action": {
    "default_title": "Click to open sidepanel",
    "default_icon": "assets/icons/icon.png"
  },
  "background": {
    "service_worker": "src/background.js"
  },
  "permissions": ["storage", "scripting", "activeTab", "sidePanel", "tabs"],
  "commands": {
    "open-side-panel": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Ctrl+B",
        "windows": "Ctrl+B"
      },
      "description": "Open side panel"
    }
  }
}
